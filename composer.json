{
    "type": "project",
    "license": "AGPL-3.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "platform": {
            "php": "8.2.0"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Donkfather/zotero-api"
        },
        {
            "type": "vcs",
            "url": "https://github.com/burki/tei-edition-bundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/igdj/lod-service"
        },
        {
            "type": "vcs",
            "url": "https://github.com/burki/converter"
        },
        {
            "type": "vcs",
            "url": "https://github.com/burki/LibRIS"
        },
        {
            "type": "vcs",
            "url": "https://github.com/burki/SolrBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/burki/LiipUrlAutoConverterBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/burki/scssphp-bundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/jtl-software/connector-iso-639"
        },
        {
            "type": "vcs",
            "url": "https://github.com/burki/FeedBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/burki/FluentDOM"
        },
        {
            "type": "vcs",
            "url": "https://github.com/burki/JMSI18nRoutingBundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/burki/scssphp-bundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/burki/OaiPmh"
        },
        {
            "type": "vcs",
            "url": "https://github.com/burki/minimal-mark"
        },
        {
            "type": "vcs",
            "url": "https://github.com/burki/wordpress-rest-api-client"
        },
        {
            "type": "package",
            "package": {
                "name": "citation-style-language/styles-distribution",
                "version": "1.0.0",
                "source": {
                    "type": "git",
                    "url": "https://github.com/citation-style-language/styles-distribution",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "armin/scssphp-bundle": "dev-master",
        "eko/feedbundle": "dev-symfony-8 as 2.3",
        "fluentdom/fluentdom": "dev-master as 8.0",
        "gjuric/i18n-routing-bundle": "dev-master as 3.2.8",
        "gmo/iso-639": "dev-master#4834357 as 1.0",
        "hedii/zotero-api": "dev-master#c296090a23e557bd7b38c7f10132ada6bc333631",
        "igdj/tei-edition-bundle": "dev-attributes",
        "jms/translation-bundle": "2.x-dev as 2.6",
        "mmz-potsdam/minimal-mark": "dev-master",
        "novaway/feature-flag-bundle": "^3.0",
        "presta/sitemap-bundle": "^4.2",
        "square-bit/oai-pmh": "dev-master as 0.3",
        "symfony/apache-pack": "^1.0",
        "symfony/dotenv": "7.4.*",
        "symfony/flex": "^2",
        "symfony/runtime": "7.4.*",
        "symfony/security-bundle": "7.4.*",
        "textrazor/textrazor-php": "*@dev",
        "vnn/wordpress-rest-api-client": "dev-master"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.70",
        "phpstan/phpstan": "^2.1",
        "symfony/debug-bundle": "7.4.*",
        "symfony/maker-bundle": "^1.43",
        "symfony/stopwatch": "7.4.*",
        "symfony/web-profiler-bundle": "7.4.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.4.*"
        }
    }
}
